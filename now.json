{
  "version": 2,
  "name": "EduScout",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api.js",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "Cache-Control": "s-maxage=0"
      }
    }
  ],
  "functions": {
    "api.js": {
      "maxDuration": 60
    }
  }
}
